public class CelsiusToFahrenheit {
    public Double celsius {get; set;}
    public Double fahrenheit {get; set;}
    
    public pagereference Temperature() {
        fahrenheit = (celsius * 1.8) + 32;
        return null;
    }
    public pagereference clear(){
        celsius = null;
        fahrenheit = null;
        return null;
    }
}